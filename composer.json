{
  "name": "26b/post-version",
  "description": "A WordPress Plugin to version your posts.",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://github.com/26B/post-version",
  "license": "GPL-2.0+",
  "require": {
    "php": ">=8.0",
    "composer/installers": "~1.0",
    "dg/composer-cleaner": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.28"
  },
  "authors": [],
  "autoload": {
    "psr-4": {
      "TwentySixB\\WP\\Plugin\\PostVersion\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwentySixB\\WP\\Plugin\\PostVersion\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "dg/composer-cleaner": true
    }
  }
}
